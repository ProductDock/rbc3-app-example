{
    "family": "ecs-eu-west-1-nonprod",
    "containerDefinitions": [
        {
            "name": "rbc-lib",
            "image": "will-be-replaced-by-cicd",
            "cpu": 512,
            "memory": 512,
            "portMappings": [
                {
                    "name": "rbc-lib",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "linuxParameters": {
                "initProcessEnabled": false
            },
            "startTimeout": 30,
            "stopTimeout": 120,
            "user": "0",
            "privileged": false,
            "readonlyRootFilesystem": false,
            "interactive": false,
            "pseudoTerminal": false,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/aws/ecs/ecs-eu-west-1-nonprod/rbc-lib",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::654654270781:role/ecs-eu-west-1-nonprod-20240801160958955200000003",
    "executionRoleArn": "arn:aws:iam::654654270781:role/ecs-eu-west-1-nonprod-20240801160958953400000002",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "1024",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": [
        {
            "key": "Owner",
            "value": "rbc"
        },
        {
            "key": "GithubRepo",
            "value": "rbc3-infra-aws"
        },
        {
            "key": "GithubOrg",
            "value": "ProductDock"
        }
    ]
}
